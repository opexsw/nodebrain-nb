makeinfo --html --no-split --number-sections nb_snmptrap.texi
makeinfo --plaintext --no-split --number-sections --output=nb_snmptrap.txt nb_snmptrap.texi
makeinfo --docbook --no-split --number-sections --output=nb_snmptrapDocBook.xml nb_snmptrap.texi
makeinfo --xml --no-split --number-sections nb_snmptrap.texi
makeinfo --no-split --number-sections nb_snmptrap.texi

./texi4pdf nb_snmptrap.texi > nb_snmptrap.texi4pdf
texi2pdf -D -V -o nb_snmptrap.pdf nb_snmptrap.texi4pdf
