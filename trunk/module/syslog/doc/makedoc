makeinfo --html --no-split --number-sections --css-include=../../doc/style.css nb_syslog.texi
makeinfo --plaintext --no-split --number-sections --output=nb_syslog.txt nb_syslog.texi
makeinfo --docbook --no-split --number-sections --output=nb_syslog_DocBook.xml nb_syslog.texi
makeinfo --xml --no-split --number-sections nb_syslog.texi
makeinfo --no-split --number-sections nb_syslog.texi
texi2pdf -D -V -o nb_syslog.pdf nb_syslog.texi
