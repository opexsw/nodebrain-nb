makeinfo --html --no-split --number-sections --css-include=../../doc/style.css nb_audit.texi
makeinfo --plaintext --no-split --number-sections --output=nb_audit.txt nb_audit.texi
makeinfo --docbook --no-split --number-sections --output=nb_audit_DocBook.xml nb_audit.texi
makeinfo --xml --no-split --number-sections nb_audit.texi
makeinfo --no-split --number-sections nb_audit.texi
texi2pdf -D -V -o nb_audit.pdf nb_audit.texi
