makeinfo --html --no-split --number-sections nbTutorial.texi
makeinfo --plaintext --no-split --number-sections --output=nbTutorial.txt nbTutorial.texi
makeinfo --docbook --no-split --number-sections --output=nbTutorialDocBook.xml nbTutorial.texi
makeinfo --xml --no-split --number-sections nbTutorial.texi
makeinfo --no-split --number-sections nbTutorial.texi

./texi4pdf nbTutorial.texi > nbTutorial.texi4pdf
texi2pdf -o nbTutorial.pdf nbTutorial.texi4pdf
