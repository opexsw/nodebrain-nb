#!/usr/bin/nb -d
# File: tutorial/Webster/webster2
-rm log/webster.log
set log="log/webster.log";
define webster node webster;
webster. define uri cell "http://0.0.0.0:62443";
webster. define Authenticate cell "password";   # Default is "yes"
webster. define AccessList   cell "security/AccessList.conf"; # Default is "security/AccessList.conf"
set traceWebster;
