makeinfo --html --no-split --number-sections --css-include=../../model/doc/style.css nb_pipe.texi
makeinfo --plaintext --no-split --number-sections --output=nb_pipe.txt nb_pipe.texi
makeinfo --docbook --no-split --number-sections --output=nb_pipe_DocBook.xml nb_pipe.texi
makeinfo --xml --no-split --number-sections nb_pipe.texi
makeinfo --no-split --number-sections nb_pipe.texi
texi2pdf -D -V -o nb_pipe.pdf nb_pipe.texi
