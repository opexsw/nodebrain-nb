~ > #
alert x;
~ > alert x;
undefine x;
~ > undefine x;
alert x;
~ > alert x;
undefine x;
~ > undefine x;
define x node;
~ > define x node;

alert a=1,b=2,x.c=3,d=4;
~ > alert a=1,b=2,x.c=3,d=4;
show -t
~ > show -t
~ _ = # == node 
~ a = 1
~ b = 2
~ d = 4
~ x = # == node 
~ x.c = 3
