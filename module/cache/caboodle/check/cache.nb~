define alder node cache:(~(1m):x{5},y[2],z(2,4,6));
~ > define alder node cache:(~(1m):x{5},y[2],z(2,4,6));
alder. define p cell;
~ > alder. define p cell;
alder. define r0 if(x._rowState):$ # x row ${x},${y},${z} ${x._rowState} ${p}
~ > alder. define r0 if(x._rowState):$ # x row ${x},${y},${z} ${x._rowState} ${p}
alder. define r1 if(y._kidState):$ # y kid ${x},${y},${z} ${y._kidState} ${p}
~ > alder. define r1 if(y._kidState):$ # y kid ${x},${y},${z} ${y._kidState} ${p}
alder. define r2 if(z._hitState):$ # z hit ${x},${y},${z} ${z._hitState} ${p}
~ > alder. define r2 if(z._hitState):$ # z hit ${x},${y},${z} ${z._hitState} ${p}
alder. alert (1,2,3),p=1;
